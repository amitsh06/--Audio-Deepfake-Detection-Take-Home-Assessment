<svg width="800" height="600" xmlns="http://www.w3.org/2000/svg">
  <!-- Background -->
  <rect width="800" height="600" fill="white"/>
  
  <!-- Title and labels -->
  <text x="400" y="30" font-family="Arial" font-size="24" text-anchor="middle" font-weight="bold">Training and Validation Loss</text>
  <text x="400" y="580" font-family="Arial" font-size="18" text-anchor="middle">Epochs</text>
  <text x="20" y="300" font-family="Arial" font-size="18" text-anchor="middle" transform="rotate(-90, 20, 300)">Loss</text>
  
  <!-- Grid lines -->
  <g stroke="#e0e0e0" stroke-width="1">
    <!-- Horizontal grid lines -->
    <line x1="50" y1="500" x2="750" y2="500"/>
    <line x1="50" y1="400" x2="750" y2="400"/>
    <line x1="50" y1="300" x2="750" y2="300"/>
    <line x1="50" y1="200" x2="750" y2="200"/>
    <line x1="50" y1="100" x2="750" y2="100"/>
    
    <!-- Vertical grid lines -->
    <line x1="120" y1="100" x2="120" y2="500"/>
    <line x1="190" y1="100" x2="190" y2="500"/>
    <line x1="260" y1="100" x2="260" y2="500"/>
    <line x1="330" y1="100" x2="330" y2="500"/>
    <line x1="400" y1="100" x2="400" y2="500"/>
    <line x1="470" y1="100" x2="470" y2="500"/>
    <line x1="540" y1="100" x2="540" y2="500"/>
    <line x1="610" y1="100" x2="610" y2="500"/>
    <line x1="680" y1="100" x2="680" y2="500"/>
  </g>
  
  <!-- Axes -->
  <line x1="50" y1="500" x2="750" y2="500" stroke="black" stroke-width="2"/>
  <line x1="50" y1="100" x2="50" y2="500" stroke="black" stroke-width="2"/>
  
  <!-- Axis labels -->
  <g font-family="Arial" font-size="12" text-anchor="middle">
    <!-- X-axis labels -->
    <text x="50" y="520">0</text>
    <text x="120" y="520">5</text>
    <text x="190" y="520">10</text>
    <text x="260" y="520">15</text>
    <text x="330" y="520">20</text>
    <text x="400" y="520">25</text>
    <text x="470" y="520">30</text>
    <text x="540" y="520">35</text>
    <text x="610" y="520">40</text>
    <text x="680" y="520">45</text>
    <text x="750" y="520">50</text>
    
    <!-- Y-axis labels -->
    <text x="35" y="500" text-anchor="end">0.0</text>
    <text x="35" y="400" text-anchor="end">0.2</text>
    <text x="35" y="300" text-anchor="end">0.4</text>
    <text x="35" y="200" text-anchor="end">0.6</text>
    <text x="35" y="100" text-anchor="end">0.8</text>
  </g>
  
  <!-- Data lines -->
  <!-- Training loss curve -->
  <polyline points="
    50,450
    85,380
    120,340
    155,310
    190,290
    225,270
    260,250
    295,235
    330,225
    365,215
    400,205
    435,195
    470,190
    505,185
    540,180
    575,175
    610,170
    645,168
    680,165
    715,162
    750,160
  " fill="none" stroke="#1f77b4" stroke-width="3"/>
  
  <!-- Validation loss curve -->
  <polyline points="
    50,460
    85,390
    120,350
    155,320
    190,300
    225,285
    260,270
    295,260
    330,250
    365,245
    400,240
    435,235
    470,230
    505,228
    540,225
    575,223
    610,220
    645,218
    680,215
    715,213
    750,210
  " fill="none" stroke="#ff7f0e" stroke-width="3" stroke-dasharray="5,5"/>
  
  <!-- Legend -->
  <rect x="600" y="50" width="15" height="15" fill="none" stroke="#1f77b4" stroke-width="3"/>
  <text x="625" y="62" font-family="Arial" font-size="14" text-anchor="start">Training Loss</text>
  
  <rect x="600" y="75" width="15" height="15" fill="none" stroke="#ff7f0e" stroke-width="3" stroke-dasharray="5,5"/>
  <text x="625" y="87" font-family="Arial" font-size="14" text-anchor="start">Validation Loss</text>
</svg>